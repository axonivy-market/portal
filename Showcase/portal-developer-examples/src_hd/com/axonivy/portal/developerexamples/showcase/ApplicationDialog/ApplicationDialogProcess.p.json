        "security" : "system",
        "output" : {
          "code" : [
            "try{",
            "  in.gdprRequest.files.remove(in.selectedGdprFile);",
            "  in.selectedGdprFile.getFile().getJavaFile().delete();",
            "  in.selectedGdprFile= null;",
            "  in.renderPdfVeiwer = false;",
            "}",
            "catch(Exception e){",
            "  ivy.log.error(\"File was not deleted, \", e);",
            "}"
          ]

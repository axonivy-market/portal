      "type" : "Script",
      "name" : "Load data",
      "config" : {
        "output" : {
          "code" : [
            "import com.axonivy.portal.components.util.CaseUtils;",
            "import ch.ivyteam.ivy.workflow.ICase;",
            "import ch.ivyteam.ivy.security.ISecurityContext;",
            "import com.axonivy.portal.components.dto.UserDTO;",
            "import com.axonivy.portal.components.examples.leaverequest.LeaveRequest;",
            "",
            "ICase caze = CaseUtils.findCase(in.caseId);",
            "in.leaveRequestData = ivy.repo.find(caze.getBusinessCase(), LeaveRequest.class) as LeaveRequest;",
            "in.requester = new UserDTO(ISecurityContext.current().users().find(in.leaveRequestData.requesterUsername));",
            "in.approver = new UserDTO(ISecurityContext.current().users().find(in.leaveRequestData.approverUsername));"
          ]
        },
        "sudo" : true
      },
      "visual" : {
        "at" : { "x" : 336, "y" : 96 }
      },
      "connect" : [
        { "id" : "f7", "to" : "f1", "color" : "default" }
      ]
    }, {
      "id" : "f8",
      "type" : "HtmlDialogEventStart",
      "name" : "submit",
      "config" : {
        "guid" : "1966C0E6558A30D7"

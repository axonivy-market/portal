      },
      "connect" : [
        { "id" : "f9", "to" : "f10" }
      ]
    }, {
      "id" : "f10",
      "type" : "Script",
      "name" : "validation",
      "config" : {
        "output" : {
          "code" : [
            "import com.axonivy.portal.components.util.FacesMessageUtils;",
            "import javax.faces.application.FacesMessage;",
            "import javax.faces.context.FacesContext;",
            "in.isValid = in.leaveRequestData.from.compareTo(in.leaveRequestData.to) > 0? false : true;",
            "if(!in.isValid) {",
            "FacesContext.getCurrentInstance().validationFailed();",
            "FacesContext.getCurrentInstance().addMessage(",
            "          null,",
            "          FacesMessageUtils.sanitizedMessage(FacesMessage.SEVERITY_ERROR, ivy.cms.co(",
            "              \"/ch.ivy.addon.portalkit.ui.jsf/common/dateFromBiggerThanTo\"), null));",
            "}"
          ]
        },
        "sudo" : true
      },
      "visual" : {
        "at" : { "x" : 232, "y" : 264 }
      },
      "connect" : [
        { "id" : "f11", "to" : "f12" }
      ]
    }, {

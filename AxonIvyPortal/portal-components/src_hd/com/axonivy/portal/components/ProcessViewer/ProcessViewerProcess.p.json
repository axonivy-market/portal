    }, {
      "id" : "f6",
      "type" : "Script",
      "name" : "Init ProcessViewer",
      "config" : {
        "security" : "system",
        "output" : {
          "code" : [
            "import ch.ivyteam.ivy.workflow.start.IWebStartable;",
            "import org.apache.commons.lang3.StringUtils;",
            "import com.axonivy.portal.components.util.ProcessViewerUtils;",
            "import com.axonivy.portal.components.util.CaseUtils;",
            "",
            "in.isError = false;",
            "if (in.caseId == 0 && StringUtils.isBlank(in.processId)) {",
            "    // Show error",
            "    in.isError = true;",
            "} else {",
            "  in.selectedCase = CaseUtils.findCase(in.caseId);",
            "",
            "  IWebStartable webStartable = ProcessViewerUtils.findWebStartable(in.caseId, in.processId);",
            "  if (webStartable == null) {",
            "    // Show error",
            "    in.isError = true;",
            "  } else {",
            "    in.webStartable = webStartable;",
            "    ",
            "    in.webLink = ProcessViewerUtils.getViewerWebLink(in.caseId, in.processId);",
            "    if (in.webLink == null) {",
            "      // Show error",
            "      in.isError = true;",
            "    }",
            "  }",
            "}"
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 224, "y" : 160 }
      },
      "connect" : { "id" : "f15", "to" : "f8" }
    }, {
      "id" : "f10",
      "type" : "Script",
      "name" : "Init info",
      "config" : {
        "output" : {
          "code" : [
            "in.activator = in.webStartable.getActivator();",
            "if (in.activator != null) {",
            "  in.activatorName = in.activator.getDisplayName();",
            "}",
            "",
            "if (in.webStartable.getCategory() != null) {",
            "  in.category = in.webStartable.getCategory().getName();",
            "}"
          ]
        }
      },

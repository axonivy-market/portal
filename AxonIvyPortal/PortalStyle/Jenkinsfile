pipeline {
  agent any
  
  options {
    buildDiscarder(logRotator(numToKeepStr: '60', artifactNumToKeepStr: '5'))
  }
  
  triggers {
    pollSCM 'H/15 * * * *'
  }

  tools { 
      maven 'default' 
      jdk 'jdk11' 
  }

  stages {
    stage('build') {
      steps {
        script {
          bat 'mvn clean install -f pom.xml'
        }
      }
    }
  }
}



pipeline {
  agent any
  
  options {
    buildDiscarder(logRotator(numToKeepStr: '60', artifactNumToKeepStr: '5'))
  }
  
  triggers {
    pollSCM '@hourly'
    cron '@midnight'
  }

  tools { 
      maven 'Maven 3.6.1' 
      jdk 'jdk8' 
  }

  stages {
    stage('build') {
      steps {
        script {
          bat 'echo 123'
          //bat 'mvn clean verify -f AxonIvyPortal/PortalStyle/pom.xml'
          maven cmd: 'clean verify -f AxonIvyPortal/PortalStyle/pom.xml'
        }
      }
    }

  }
}



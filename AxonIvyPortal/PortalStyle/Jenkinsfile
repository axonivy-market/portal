  agent any
  
  options {
    buildDiscarder(logRotator(numToKeepStr: '60', artifactNumToKeepStr: '5'))
  }
  
  triggers {
    pollSCM '@hourly'
    cron '@midnight'
  }

  stages {
    stage('build') {
      steps {
        script {
          bat 'echo 123'
          maven cmd: "clean verify"
          asd fasdf sadf asdfasfd
        }
      }
    }

  }
}



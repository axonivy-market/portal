<?xml version="1.0" encoding="utf-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>ch.cs.ivy</groupId>
  <artifactId>portalTemplate</artifactId>
  <version>1.0.0-m1</version>
  <packaging>iar</packaging>
  <properties>
    <build.plugin.version>8.0.5</build.plugin.version>
    <ivy.engine.version>8.0.8</ivy.engine.version>
  </properties>
  <distributionManagement>
    <repository>
      <id>central</id>
      <name>repo.axonivy.io</name>
      <url>https://repo.axonivy.io/artifactory/ext-release-local</url>
    </repository>
  </distributionManagement>
  <dependencies>
    <dependency>
      <groupId>ch.cs.ivy</groupId>
      <artifactId>portalKit</artifactId>
      <version>8.0.8</version>
      <type>iar</type>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>com.axonivy.ivy.ci</groupId>
        <artifactId>project-build-plugin</artifactId>
        <version>${build.plugin.version}</version>
        <extensions>true</extensions>
        <configuration>
          <compilerWarnings>false</compilerWarnings>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
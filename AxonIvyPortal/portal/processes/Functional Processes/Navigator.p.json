        "security" : "system",
        "output" : {
          "code" : [
            "import ch.ivyteam.ivy.workflow.ICase;",
            "import ch.ivy.addon.portalkit.util.PermissionUtils;",
            "import ch.ivyteam.ivy.workflow.TaskState;",
            "import ch.ivy.addon.portalkit.util.PermissionUtils;",

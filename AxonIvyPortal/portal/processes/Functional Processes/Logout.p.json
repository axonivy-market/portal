{
  "$schema" : "https://json-schema.axonivy.com/process/11.1.26/process.json",
  "id" : "15EF0843F1DB39F2",
  "kind" : "CALLABLE_SUB",
  "config" : {
    "data" : "ch.ivy.add.portalkit.LogoutData"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "CallSubStart",
      "name" : "call(Boolean,ITask)",
      "config" : {
        "signature" : "call",
        "input" : {
          "params" : [
            { "name" : "isTaskReserve", "type" : "Boolean", "desc" : "" },
            { "name" : "task", "type" : "ch.ivyteam.ivy.workflow.ITask", "desc" : "" }
          ],
          "map" : {
            "out.isTaskReserve" : "param.isTaskReserve",
            "out.task" : "param.task"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 96, "y" : 64 },
        "labelOffset" : { "x" : 25, "y" : 37 }
      },
      "connect" : [
        { "id" : "f4", "to" : "f3" }
      ]
    }, {
      "id" : "f1",
      "type" : "CallSubEnd",
      "visual" : {
        "at" : { "x" : 368, "y" : 64 }
      }
    }, {
      "id" : "f3",
      "type" : "Script",
      "name" : "Logout",
      "config" : {
        "output" : {
          "code" : [
            "import ch.ivy.addon.portalkit.util.SecurityServiceUtils;",
            "import ch.ivy.addon.portalkit.enums.SessionAttribute;",
            "if (in.isTaskReserve) {",
            "  ivy.session.parkTask(in.#task != null ? in.task : ivy.task);",
            "}",
            "ivy.session.logoutSessionUser();",
            "",
            "",
            "for (SessionAttribute item: SessionAttribute.values()){",
            "  SecurityServiceUtils.removeSessionAttribute(item.toString());",
            "}"
          ]
        },
        "sudo" : true
      },
      "visual" : {
        "at" : { "x" : 240, "y" : 64 }
      },
      "connect" : [
        { "id" : "f2", "to" : "f1" }
      ]
    } ]
}
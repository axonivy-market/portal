    "permissions": [
      "Everybody"
    ],
    "chartType": "bar",
    "label": "Tasks grouped by expiry",
    "barChartConfig": {
       "xTitle": "Day",
       "yTitle": "Number of tasks"
     }
  },
  {
    "id": "6",
    "aggregates": "endTimestamp",
    "filter": "endTimestamp:>=now-4d/d <=now+1d,businessState:DONE",

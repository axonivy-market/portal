      "Everybody"
    ] ,
    "chartTarget": "TASK",
    "chartType": "bar",
    "label": "Completed Tasks per Day",
    "barChartConfig": {
       "xTitle": "Day",
       "yTitle": "Number of tasks"
     }
  },

            "import java.util.HashMap;",
            "import java.util.Map;",
            "import com.axonivy.portal.components.enums.PortalCustomSignature;",
            "import com.axonivy.portal.components.service.IvyAdapterService;",
            "",
            "Map params = new HashMap();",
            "params.put(\"newPassword\", in.newPassword);",
            "params.put(\"passwordConfirmation\", in.passwordConfirmation);",
            "params.put(\"token\", in.token);",
            "params.put(\"username\", in.username);",
            "",
            "in.result = IvyAdapterService.startSubProcessInSecurityContextWithDefault(",
            "  PortalCustomSignature.RESET_PASSWORD.getSignature(),",
            "  params,",
            "  PortalCustomSignature.RESET_PASSWORD.getDefaultSignature());",
            "",
            "if (in.result != null && in.result.get(\"callProcessError\") == null) {",
            " in.message = in.result.get(\"message\") as String;",
            " in.resetSuccess = in.result.get(\"resetSuccess\") as Boolean;",
            "}"
          ]
        }

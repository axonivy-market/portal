          "type" : "Script",
          "name" : [
            "Check if can",
            "delegate task"
          ],
          "config" : {
            "output" : {
              "code" : [
                "import ch.ivyteam.ivy.security.IRole;",
                "import com.axonivy.portal.components.dto.RoleDTO;",
                "import ch.ivyteam.ivy.security.IUser;",
                "import ch.ivyteam.ivy.security.ISecurityMember;",
                "import ch.ivyteam.ivy.workflow.task.expiry.responsible.ExpiryResponsible;",
                "import ch.ivy.addon.portalkit.util.TaskUtils;",
                "import com.axonivy.portal.components.dto.UserDTO;",
                "",
                "in.canChangeTaskActivator = !(in.roles.isEmpty() && in.users.isEmpty());",
                "",
                "if (in.canChangeTaskActivator) {",
                "  List<ExpiryResponsible> expiryResponsibleList = in.task.expiry().responsibles().all();",
                "  if (expiryResponsibleList.size() > 0) {",
                "      ISecurityMember securityMember = expiryResponsibleList.get(0).get();",
                "        if (securityMember.isUser()) {",
                "          IUser user = securityMember as IUser;",
                "          UserDTO userDTO = new UserDTO(user);",
                "          in.selectedUser = userDTO;",
                "          in.isUserAssigned = true;",
                "        } else {",
                "          IRole role = securityMember as IRole;",
                "          RoleDTO roleDTO = new RoleDTO(role);",
                "          in.selectedRole = roleDTO;",
                "          in.isUserAssigned = false;",
                "        }",
                "  } else {",
                "      in.isUserAssigned = true;",
                "  }",
                "}"
              ]
            }
          },

      "name" : "start(String)",
      "config" : {
        "signature" : "start",
        "input" : {
          "params" : [
            { "name" : "processId", "type" : "String", "desc" : "" }
          ],
          "map" : {
            "out.processId" : "param.processId"
          }
        },
        "guid" : "17982F86F32B7C49"
      },
      "visual" : {
        "at" : { "x" : 96, "y" : 64 },
        "size" : { "width" : 24, "height" : 24 },
        "labelOffset" : { "x" : 24, "y" : 35 }
      },
      "connect" : [
